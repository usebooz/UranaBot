{
  "name": "uranabot",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "node --test --import tsx tests/**/*.test.ts",
    "test:watch": "node --test --watch --import tsx tests/**/*.test.ts",
    "test:debug": "node --inspect-brk --test --import tsx tests/**/*.test.ts",
    "test:unit": "node --test --import tsx tests/unit/**/*.test.ts",
    "test:integration": "node --test --import tsx tests/integration/**/*.test.ts",
    "test:coverage": "node --test --experimental-test-coverage --test-coverage-exclude='tests/**' --import tsx tests/**/*.test.ts",
    "test:ci": "SKIP_INTEGRATION_TESTS=true node --test --experimental-test-coverage --test-coverage-exclude='tests/**' --import tsx tests/**/*.test.ts",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:fix": "npm run codegen && node scripts/fix-generated-imports.js",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch"
  },
  "keywords": [
    "telegram",
    "bot",
    "grammy",
    "typescript",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.1",
    "grammy": "^1.37.0",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/client-preset": "^4.8.3",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
